{
  "$schema": "http://json.schemastore.org/webcleanarchitecture",
  "author": "Asier",
  "identity": "clean-architecture",
  "name": "Arquitectura limpia web",
  "shortName": "WebCleanArchitecture",
  "sourceName": "WebCleanArchitecture",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net8.0"
        },
        {
          "choice": "net9.0"
        }
      ],
      "defaultValue": "net9.0"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/*template*",
            "**/*idea"
          ]
        }
      ]
    }
  ]
}