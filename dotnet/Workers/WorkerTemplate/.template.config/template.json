{
  "$schema": "http://json.schemastore.org/template",
  "author": "Asier",
  "classifications": [
    "architecture"
  ],
  "identity": "Worker-Service",
  "name": "Worker Services",
  "description": "Crear Worker Service",
  "shortName": "Worker Service",
  "sourceName": "WorkerTemplate",
  "language": "C#",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net9.0"
        }
      ],
      "defaultValue": "net9.0"
    },
    "RabbitMq": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "Rabbit MQ Handler",
      "defaultValue": false
    },
    "AzServiceBus": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "Azure Service Bus Handler",
      "defaultValue": false
    },
    "Mongodb": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "Mongo Database",
      "defaultValue": false
    },
    "Postgres": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "Ef core para postgres",
      "defaultValue": false
    }
  },
  "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        "**/*template.config/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*idea",
        "*.sln",
        "**/CleanArchitecture/**/*"
      ],
      "modifiers": [
      ]
    },
    {
      "condition": "(!UseGrpc)",
      "exclude": [
        "**/CleanArchitecture.Grpc/**/*"
      ],
      "rename": {
      }
    }
  ]
}